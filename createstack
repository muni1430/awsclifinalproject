    stages{
       stage('hosting application'){
        steps{
          sh "ls"
           sh "aws cloudformation create-stack --stack-name mystack1 --template-body file://awscli/stack.yml --parameters ParameterKey=newawskey,ParameterValue=TestKey --region us-east-1" 
        }
       }
       }
       }
       "Version":"2012-10-17",
    "Statement":[{
        "Effect":"Allow",
        "Action":[
            "cloudformation:CreateStack",
            "cloudformation:DescribeStacks",
            "cloudformation:DescribeStackEvents",
            "cloudformation:DescribeStackResources",
            "cloudformation:GetTemplate",
            "cloudformation:ValidateTemplate"  
        ],
       "Resource":"arn:aws:iam::362480651898:user/myuser"
    }]
     }
   }
      
